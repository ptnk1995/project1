<tr>
  <td><%= l object.created_at, format: :short %></td>
  <td><%= object.category.name %></td>
  <td>
    <span class="label label-<%= show_status_exam object %>">
      <%= object.status %>
    </span>
  </td>
  <td><%= Settings.exams.duaration %></td>
  <td><%= Settings.exams.number_question %></td>
  <td class="center">
    <%= object.checked? ? "#{object.mark}/#{object.question_exams.count}" : "-" %>
  </td>
  <% if object.start? %>
    <td>
      <%= link_to t("start"), exam_path(object),
        class: "btn btn-primary" %>
    </td>
  <% else %>
    <td>
      <%= link_to t("view"), exam_path(object),class: "btn btn-success" %>
    </td>
  <% end %>
</tr>
